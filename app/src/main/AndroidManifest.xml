<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mark.customview">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QQStepViewActivity" />
        <activity android:name=".MultiShapeProgressActivity" />
        <activity android:name=".OverWatchProgressViewActivity" />
        <activity android:name=".LetterIndexBarActivity" />
        <activity android:name=".FlowLayoutActivity" />
        <activity android:name=".TagFlowLayoutActivity" />
        <activity android:name=".SlidingMenuActivity" />
        <activity android:name=".QQSlidingMenuActivity" />
        <activity android:name=".DragListViewActivity" />
        <activity android:name=".TestDragViewActivity" />
        <activity android:name=".SquaredLatticeLockActivity" />
        <activity
            android:name=".MaterialDesign.TestMDNavigationViewActivity"
            android:label="@string/title_activity_test_md"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MaterialDesign.TestCollapsingToolbarLayoutActivity"
            android:label="@string/title_activity_test_collapsing_toolbar_layout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MaterialDesign.CustomBehaviorActivity"
            android:label="@string/title_activity_custom_behavior"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".ItemSwipeMenuLayoutActivity" />
        <activity android:name=".RecyclerViewDividerActivity" />
        <activity android:name=".CustomLayoutManagerActivity" />
        <activity android:name=".CardLayoutManagerActivity" />
        <activity android:name=".BannerLayoutManagerActivity" />
        <activity android:name=".SourceLinearLayoutManagerActivity" />
    </application>

</manifest>